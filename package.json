{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start:client:dev": "next dev apps/client",
    "build:client": "next build apps/client",
    "start:client": "next start dist",
    "test:client": "jest --config ./apps/client/jest.config.js",
    "test:client:watch": "jest --config ./apps/client/jest.config.js --watch",
    "test:client:coverage": "jest --config ./apps/client/jest.config.js --coverage --runInBand",
    "start:mock-server": "node dist/mock-server/index.js",
    "start:apollo-server": "node dist/apollo-server/server.js",
    "start:mock-server:dev": "nodemon 'apps/mock-server/src/index.ts' --exec 'ts-node' src/index.ts -e ts",
    "start:apollo-server:dev": "nodemon 'apps/apollo-server/src/server.ts' --exec ts-node src/server.ts -e ts,graphql",
    "build:mock-server": "tsc -p apps/mock-server && ncp apps/mock-server/src/images dist/mock-server/images",
    "build:apollo-server": "tsc -p apps/apollo-server && ncp apps/apollo-server/src/schema dist/apollo-server/schema",
    "test:apollo-server": "jest --config apps/apollo-server/jest.config.js",
    "test:apollo-server:coverage": "yarn test:apollo-server --coverage --runInBand",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config .prettierrc"
  },
  "dependencies": {
    "@apollo/client": "^3.2.5",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "graphql": "^15.3.0",
    "next": "9.5.5",
    "next-images": "^1.6.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.2.0",
    "apollo-server-express": "^2.18.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "http": "^0.0.1-security",
    "immutable": "^4.0.0-rc.12",
    "jimp": "^0.16.1",
    "ncp": "^2.0.0",
    "node-fetch": "^2.6.1",
    "ts-md5": "^1.2.7"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "@babel/plugin-transform-reserved-words": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/plugin-transform-typescript": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-flow": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@testing-library/react": "^11.1.0",
    "@testing-library/react-hooks": "^3.4.2",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.1",
    "@types/react": "^16.9.53",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "babel-jest": "^26.6.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-inline-react-svg": "^1.1.1",
    "babel-plugin-styled-components": "^1.11.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-react-app": "^9.1.2",
    "jest": "^26.6.0",
    "jest-styled-components": "^7.0.3",
    "prettier": "^2.1.2",
    "react-test-renderer": "^17.0.0",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3",
    "@jagi/jest-transform-graphql": "^1.0.2",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.8",
    "@types/graphql": "^14.5.0",
    "@types/graphql-depth-limit": "^1.1.2",
    "graphql-depth-limit": "^1.1.0",
    "graphql-import": "^1.0.2",
    "graphql-import-node": "^0.0.4",
    "jest-fetch-mock": "^3.0.3",
    "nodemon": "^2.0.6",
    "ts-loader": "^8.0.6",
    "ts-node": "^9.0.0"
  }
}
